<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageId>WinFormsControlLibrary.RangePicker</PackageId>
    <Version>1.0.0</Version>
    <Authors>Yevhenii Heoka</Authors>
    <Company>612pst</Company>
    <PackageTags>Range,Picker</PackageTags>
    <Description>This package will add the RangePicker component that allows users to select a certain interval in given range.</Description>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="RangePicker.bmp" />
    <None Remove="RangePicker.ico" />
    <None Remove="Sprite-0001.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="RangePicker.bmp" />
    <EmbeddedResource Include="RangePicker.ico" />
    <EmbeddedResource Include="Sprite-0001.ico" />
  </ItemGroup>

</Project>
